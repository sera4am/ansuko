    "lodash",
    "utility",
    "utilities",
    "typescript",
    "promise",
    "async",
    "japanese",
    "katakana",
    "hiragana",
    "geojson",
    "turf",
    "gis",
    "json5",
    "functional"
  ],
  "author": "Naoto Sera <sera@4am.jp>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sera4am/ansuko.git"
  },
  "bugs": {
    "url": "https://github.com/sera4am/ansuko/issues"
  },
  "homepage": "https://github.com/sera4am/ansuko#readme",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": "./dist/index.js",
